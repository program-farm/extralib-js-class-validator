{
  "name": "@program-farm/extralib-js-class-validator",
  "version": "0.0.2",
  "description": "Extra library on class-validator.js",
  "keywords": [
    "ExtraLibJS",
    "class-validator",
    "program-farm"
  ],
  "private": false,
  "files": ["lib/**/*"],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/program-farm/extralib-js-class-validator",
  "author": "TerryJoo",
  "license": "ISC",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "test": "jest",
    "format": "prettier --write \"(src|test)/**/*.(t|j)s\""
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3",
    "class-validator": "^0.13.0"
  },
  "peerDependencies": {
    "class-validator": "^0.13.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
